.lists{
  width:642rpx;
  height:auto;
  /* padding:5%; */
  background: #fff;
  margin-top:10rpx;
  margin-left:54rpx;
}
.list{
  height:100%;
  position: relative;
}
.list .list_left{
  width:10%;
  height:100%;
  background: #fed444;
  float: left;
  text-align:center;
  font-size:12px;
  padding-top:10rpx;
}
.list_right{
  width:80%;
  height:auto;
  float: left;
  font-size:12px;
  padding-top:10rpx;
}
.list_change{
  height:20px;
  width:20px;
  font-size:12px;
  position: absolute;
  top:10px;
  right: 20px;
}
.text{
  height:60rpx;
  line-height:60rpx;
  padding-left:2%;
  border-bottom:1rpx solid #eee;
}
.addInfo{
  width:170rpx;
  height:40rpx;
  line-height:40rpx;
  background:#9288d5;
  font-size:12px;
  color:#fff;
  /* position:absolute; */
  /* top:95rpx;
  left:74rpx; */
  margin:0;
  margin-left:80rpx;
}
.list_info{
  width:100%;
  height:80rpx;
  line-height:80rpx;
}