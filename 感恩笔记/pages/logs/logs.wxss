page{
  width:100%;
  height:100%;
  background:#f7f7f7;
}
.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.changebutton{
  width:20rpx;
  height:20rpx;
  background:red;
}
.lists{
  width:642rpx;
  height:200rpx;
  /* padding:5%; */
  background: #fff;
  margin-top:10rpx;
  margin-left:54rpx;
}
.list{
  height:100%;
  position: relative;
}
.list_left{
  width:10%;
  height:100%;
  background: #fed444;
  float: left;
  text-align:center;
  font-size:12px;
  padding-top:10rpx;
}
.list_right{
  width:78%;
  padding-left:2%;
  height:auto;
  float: left;
  font-size:12px;
  padding-top:10rpx;
}
.list_change{
  height:20px;
  width:20px;
  font-size:12px;
  position: absolute;
  top:10px;
  right: 20px;
}